<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\Entity\EkategorieJezyki" table="ekategorie_jezyki">
    <id name="id" type="bigint" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="idKategorii" type="bigint" column="id_kategorii" nullable="true">
      <options>
        <option name="unsigned">1</option>
      </options>
    </field>
    <field name="idJezyka" type="boolean" column="id_jezyka" nullable="true"/>
    <field name="nazwaKategorii" type="string" column="nazwa_kategorii" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="opisKategorii" type="text" column="opis_kategorii" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="title" type="string" column="title" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="description" type="string" column="description" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="keywords" type="string" column="keywords" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="naglowekSeo" type="string" column="naglowek_seo" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
